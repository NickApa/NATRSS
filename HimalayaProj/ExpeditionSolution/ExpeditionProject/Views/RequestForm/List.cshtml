@model IQueryable<ExpeditionProject.Models.Form>




<table class="table">
    <thead>
        <tr>

            <th>
                UserID
            </th>

            <th>
                ExpeditionID
            </th>
            <th>
                Submission Date
            </th>
            <th>
                Description
            </th>
            <th>
                Status
            </th>
            <th>
                Completed
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {


            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ExpeditionId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SubmissionDateTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Status)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Completed)
                </td>
                <td>
                    <a href="RequestFormReview/@item.Id">Review</a>
                </td>

            </tr>

        }
    </tbody>
</table>